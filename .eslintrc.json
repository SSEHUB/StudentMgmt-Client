{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": [
	  "@typescript-eslint"
	],
	"extends": [
	  "eslint:recommended",
	  "plugin:@typescript-eslint/eslint-recommended",
	  "plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"indent": ["error", "tab"],
		"@typescript-eslint/camelcase": "off",
		"@typescript-eslint/class-name-casing": "off"
	}
}
